        "name": "Temperature/Pressure Sensor",
        "examplePartNumber": "LPS22HB",
        "driverId": [
            "0xF007"
        ],
        "description" : "The LPS22HB sensor is a digital output barometer which detects absolute pressure, it has a range of 260 to 1260 hPa, an odr selection from 1-75Hz, as well as low-power functionality",
        "platforms": [
            "iEnable"
        ],
        "powerConsumption": "",
        "groups": [
            {
                "groupName": "General Parameters",
                "configParams": [
                    {
                        "internalName": "act",
                        "name": "Active",
                        "description" : "Set act to true to enable sensor",
                        "values": [
                            [
                                "False",
                                "VALUE_FALSE",
                                0,
                                0
                            ],
                            [
                                "True",
                                "VALUE_TRUE",
                                1,
                                1
                            ]
                        ],
                        "defaultValue": [
                            0,
                            0
                        ],
                        "guiEnabled": "True",
                        "guiControl": "checkbox"
                    },
                    {
                        "internalName": "odr",
                        "name": "ODR",
                        "description" : "Set odr(output data rate) to desired frequency",
                        "values": [
                            [
                                "Power Down",
                                "CTRL_REG1_VAL_POWER_DOWN",
                                0,
                                1
                            ],
                            [
                                "1 Hz",
                                "CTRL_REG1_VAL_ODR_1HZ",
                                1,
                                1
                            ],
                            [
                                "10 Hz",
                                "CTRL_REG1_VAL_ODR_10HZ",
                                2,
                                2
                            ],
                            [
                                "25 Hz",
                                "CTRL_REG1_VAL_ODR_25HZ",
                                3,
                                3
                            ],
                            [
                                "50 Hz",
                                "CTRL_REG1_VAL_ODR_50HZ",
                                4,
                                4
                            ],
                            [
                                "75 Hz",
                                "CTRL_REG1_VAL_ODR_75HZ",
                                5,
                                6
                            ]
                        ],
                        "defaultValue": [
                            0,
                            0
                        ],
                        "guiEnabled": "True",
                        "guiControl": "slider"
                    },
                    {
                        "internalName": "enOS",
                        "name": "Enable One-Shot",
                        "description" : "Enable One-Shot to trigger single reading on demand",
                        "values": [
                            [
                                "False",
                                "CTRL_REG2_VAL_ONE_SHOT_DIS",
                                0,
                                0
                            ],
                            [
                                "True",
                                "CTRL_REG2_VAL_ONE_SHOT_EN",
                                1,
                                0
                            ]
                        ],
                        "defaultValue": [
                            0,
                            0
                        ],
                        "guiEnabled": "True",
                        "guiControl": "checkbox"
                    },
                    {
                        "internalName": "enLP",
                        "name": "Enable Low Power Mode",
                        "values": [
                            [
                                "False",
                                "RES_CONF_VAL_LC_DIS",
                                0,
                                0
                            ],
                            [
                                "True",
                                "RES_CONF_VAL_LC_EN",
                                1,
                                0
                            ]
                        ],
                        "defaultValue": [
                            0,
                            0
                        ],
                        "guiEnabled": "odr > 0",
                        "guiControl": "checkbox"
                    }
                ]
            }
        ],
        "commands": [
            {  "condition": "odr == 0 or  enOS == 1",  "command": [ "0x00", "0x05", "0x00", "0x02", "0x00", "CTRL_REG1_VAL_POWER_DOWN" ], "comment" : "CTRL_REG1"  },
            {  "condition": "odr >  0 and enOS != 1",  "command": [ "0x00", "0x05", "0x00", "0x02", "0x00", "CTRL_REG1_VAL_PRIV_WRITE + odr" ], "comment" : "CTRL_REG1"  },

            {  "condition": "enOS == 1",               "command": [ "0x00", "0x05", "0x00", "0x02", "0x01", "CTRL_REG2_VAL_PRIV_WRITE + CTRL_REG2_VAL_ONE_SHOT_EN" ],  "comment" : "CTRL_REG2"  },
            {  "condition": "enOS != 1",               "command": [ "0x00", "0x05", "0x00", "0x02", "0x01", "CTRL_REG2_VAL_PRIV_WRITE + CTRL_REG2_VAL_ONE_SHOT_DIS" ], "comment" : "CTRL_REG2"  },

            {  "condition": "odr > 0 and enLP == 1",   "command": [ "0x00", "0x05", "0x00", "0x02", "0x03", "RES_CONF_VAL_LC_EN" ],  "comment" : "RES_CONF"  },
            {  "condition": "odr > 0 and enLP != 1",   "command": [ "0x00", "0x05", "0x00", "0x02", "0x03", "RES_CONF_VAL_LC_DIS" ], "comment" : "RES_CONF"  },

            {  "condition": "act == True",    "command": [ "0x00", "0x01", "0x00", "0x01", "0x01" ], "comment" : "ENABLE"  },
            {  "condition": "act == False",   "command": [ "0x00", "0x01", "0x00", "0x01", "0x00" ], "comment" : "DISABLE"  }
        ],
        "ports": [
            {
                "index": 0,
                "name": "Enable",
                "type": "in",
                "value": "bool"
            },
            {
                "index": 0,
                "name": "Pressure",
                "type": "out",
                "value": "data"
            }
        ]
    }
