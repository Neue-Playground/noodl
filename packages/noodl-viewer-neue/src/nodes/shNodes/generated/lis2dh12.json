            {  "condition": "odr == 0",               "command": [ "0x00", "0x05", "0x00", "0x01", "0x00", "odr" ], "comment" : "CTRL_REG1, index 0"  },

            {  "condition": "odr > 0 and enT == 1" ,  "command": [ "0x00", "0x05", "0x00", "0x01", "0x04", "TEMP_CFG_REG_VAL_TEMP_EN" ], "comment" : "TEMP_CFG_REG"  },
            {  "condition": "odr > 0 and enT != 1" ,  "command": [ "0x00", "0x05", "0x00", "0x01", "0x04", "TEMP_CFG_REG_VAL_TEMP_DIS" ], "comment" : "TEMP_CFG_REG"  },

            {  "condition": "odr > 0 and mode == 1",  "command": [ "0x00", "0x05", "0x00", "0x01", "0x00", "odr + CTRL_REG1_VAL_LPEN  + (enX + enY + enZ)" ], "comment" : "CTRL_REG1, index 0"  },
            {  "condition": "odr > 0 and mode != 1",  "command": [ "0x00", "0x05", "0x00", "0x01", "0x00", "odr + CTRL_REG1_VAL_LPDIS + (enX + enY + enZ)" ], "comment" : "CTRL_REG1, index 0"  },            
            {  "condition": "odr > 0 and mode != 3",  "command": [ "0x00", "0x05", "0x00", "0x01", "0x03", "CTRL_REG4_VAL_PRIV_WRITE + CTRL_REG4_VAL_HR_DIS + scale " ], "comment" : "CTRL_REG4, index 3"  },
